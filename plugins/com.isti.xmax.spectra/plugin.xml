<?xml version="1.0" ?> 
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 0.5" 
"http://jpf.sourceforge.net/plugin_0_5.dtd"> 
<plugin id="com.isti.xmax.spectra" version="1.0"> 
	<requires> 
		<import plugin-id="com.isti.xmax.core"/> 
	</requires> 
	<runtime> 
		<library id="spectra" path="classes/" type="code"/> 
	</runtime> 
	<extension plugin-id="com.isti.xmax.core" point-id="Transformation" id="Spectra"> 
		<parameter id="class" value="TransSpectra"/>
		<parameter id="name" value="Spectra"/> 
		<parameter id="description" value="Shows spectra for selected channels"/>
		<parameter id="max_data_length" value="262144"/>   
	</extension> 
</plugin> 